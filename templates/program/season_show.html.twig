{% extends 'base.html.twig' %}

{% block body %}

    <h1> Season {{ season.number }} of {{ program.title }}</h1>

    <h2>Episodes : </h2>
    <ul>
        {% for episode in episodes %}
            <li>
                Episode {{ episode.number }} : {{ episode.title }}
            </li>
        {% endfor %}
    </ul>

{% endblock %}